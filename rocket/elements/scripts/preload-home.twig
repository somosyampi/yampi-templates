{% set banner = pageConfig.data.sections.1.params.resource.first_banner %}

{% if banner %}
    {% if banner['image'] %}
        <link rel="preload" as="image" href="{{ banner['image'] }}" fetchpriority="high"/>
    {% endif %}
    {% if banner['mobile_image'] %}
        <link rel="preload" as="image" href="{{ banner['mobile_image'] }}" fetchpriority="high"/>
    {% endif %}

{% endif %}