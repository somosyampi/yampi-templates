{% set image = product.data.images.data.0.url %}

{% set thumbor = {
    'unsafe': true,
    'resize': '600x600'
} %}

{% set thumborMobile = thumbor|merge({'resize': '700x700'}) %}

{% if image %}
    <link rel="preload" as="image" href="{{ thumborize(image, thumbor) }}" />
    <link rel="preload" as="image" href="{{ thumborize(image, thumborMobile) }}" />
{% endif %}
